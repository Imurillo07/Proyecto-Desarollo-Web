
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>
<div class="row">

    <form action="@Url.Content("~/Login")" method="post">
        <div class= "form-group">
            <label>Correo</label>
            <input type="email" name="Correo"/>
        </div>
        <div class= "form-group">
            <label>Constraseña</label>
            <input type="password" name="pass"/>
        </div>
        <div class= "form-group">
            <input type="button" value="Entar" class="btn btn-success"
        </div>

    </form>
</div>

