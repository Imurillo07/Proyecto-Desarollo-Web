@model IEnumerable<DW.Models.tienda>

@{
    ViewBag.Title = "Aprobar";
}

<h2>Aprobar</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombre_tienda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descripcion_tienda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.telefono_tienda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.imagen_tienda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.correo_tienda)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombre_tienda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.descripcion_tienda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.telefono_tienda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.imagen_tienda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.correo_tienda)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
